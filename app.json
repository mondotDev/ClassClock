{
  "expo": {
    "name": "ClassCountdown",
    "slug": "classcountdown",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B5C9E"
    },
    "assetBundlePatterns": ["**/*"],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD" 
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.classcountdown.app" 
    },
    "android": {
      "package": "com.classcountdown.app", 
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1B5C9E"
      },
      "permissions": [] 
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "experiments": {
      "newArchEnabled": true
    },
    "extra": {
      "eas": {
        "projectId": "your-eas-project-id" 
      }
    }
  }
}
